<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Estadoconexion" table="EstadoConexion">
    <id name="idestadoconexion" type="integer" column="idEstadoConexion">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombreestadoconexion" type="string" column="nombreEstadoConexion" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="color" type="string" column="color" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
